package hierarchiallevel;

public class A {
	public void display() {
		System.out.println("I am method from class A");
	}

}
public class B {
	public void display() {
		System.out.println("I am method from class B");
	}

}

public class C {
	public void display() {
		System.out.println("I am method from class C");
	}

}
public class D {
	public void display() {
		System.out.println("I am method from class D");
	}

}
public class heirarchialmain {

	public static void main(String[] args) {
		//A a1 = new A()
		B b1 = new B();
		C c1 = new C();
		D d1 = new D();
		
		b1.display();
		c1.display();
		d1.display();
	}

}
